<%- include('partials/header'); -%>

<h1>Home</h1>
<p><%= homeStartingContent %></p>

<% posts.forEach(function(post) { %>
  <h1><%= post.titulo %></h1>
  <p>
    <%= post.post.substring(0, 100) + "..." %>
    <a href="/posts/<%= post.titulo %>">Leia mais</a>
  </p>
<% }) %>

<%- include('partials/footer'); -%>
